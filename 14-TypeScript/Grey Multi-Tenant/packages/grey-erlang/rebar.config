{erl_opts, [debug_info]}.

{deps, [
    {grpcbox, "0.17.1"},
    {jsx, "3.1.0"}
]}.

{shell, [
    {apps, [grey_sdk]}
]}.

{project_plugins, [
    {grpcbox_plugin, "0.11.0"}
]}.

{grpc, [
    {protos, "priv/protos"},
    {out_dir, "src/grpc"},
    {gpb_opts, [
        {module_name_suffix, "_pb"},
        use_packages
    ]}
]}.

{profiles, [
    {test, [
        {deps, [
            {meck, "0.9.2"}
        ]}
    ]}
]}.
