{
  "name": "@grey/next",
  "version": "0.1.0",
  "description": "Grey Multi-Tenant Next.js bindings with SSR support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@grey/adapters": "workspace:*",
    "@grey/core-client": "workspace:*",
    "@grey/react": "workspace:*"
  },
  "peerDependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "files": ["dist"],
  "license": "MIT"
}
