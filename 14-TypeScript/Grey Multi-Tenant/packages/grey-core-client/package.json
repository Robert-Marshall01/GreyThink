{
  "name": "@grey/core-client",
  "version": "0.1.0",
  "description": "TypeScript SDK for Grey Multi-Tenant API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/generated/schema.d.ts",
      "import": "./dist/generated/schema.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "generate": "openapi-typescript ../../../contracts/openapi.yaml -o src/generated/schema.ts",
    "generate:client": "npm run generate && npm run build",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "grey",
    "multi-tenant",
    "api-client",
    "sdk",
    "typescript"
  ],
  "author": "Grey Platform Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "openapi-typescript": "^7.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "openapi-fetch": "^0.12.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
