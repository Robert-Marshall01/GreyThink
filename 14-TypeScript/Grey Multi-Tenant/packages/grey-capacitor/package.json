{
  "name": "@grey/capacitor",
  "version": "0.1.0",
  "description": "Grey Multi-Tenant Capacitor bindings for hybrid mobile apps",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@grey/adapters": "workspace:*",
    "@grey/core-client": "workspace:*"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.0.0 || ^6.0.0",
    "@capacitor/preferences": "^5.0.0 || ^6.0.0",
    "react": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "@capacitor/preferences": {
      "optional": true
    }
  },
  "devDependencies": {
    "@capacitor/core": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "files": ["dist"],
  "license": "MIT"
}
