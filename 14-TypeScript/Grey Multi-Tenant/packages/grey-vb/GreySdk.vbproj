<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <RootNamespace>Grey</RootNamespace>
    <AssemblyName>GreySdk</AssemblyName>
    <Version>1.0.0</Version>
    <Authors>Grey Team</Authors>
    <Company>Grey</Company>
    <Product>Grey SDK for Visual Basic</Product>
    <Description>Grey Multi-Tenant SDK for Visual Basic .NET</Description>
    <PackageId>GreySdk.VisualBasic</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/grey/grey-multi-tenant</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>grey;sdk;multi-tenant;api;http</PackageTags>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Nullable>disable</Nullable>
    <OptionStrict>Off</OptionStrict>
    <OptionExplicit>On</OptionExplicit>
    <OptionInfer>On</OptionInfer>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="8.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="README.md" Pack="true" PackagePath="" />
    <None Include="LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
