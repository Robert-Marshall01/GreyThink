{
  "name": "@grey/sveltekit",
  "version": "0.1.0",
  "description": "Grey Multi-Tenant SvelteKit bindings with SSR support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "svelte": "src/client/GreyProvider.svelte",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./src/client/GreyProvider.svelte",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "svelte": "./src/client/GreyProvider.svelte",
      "import": "./dist/client/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@grey/adapters": "workspace:*",
    "@grey/core-client": "workspace:*",
    "@grey/svelte": "workspace:*"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "@types/node": "^20.10.0",
    "svelte": "^4.0.0",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "files": ["dist", "src"],
  "license": "MIT"
}
