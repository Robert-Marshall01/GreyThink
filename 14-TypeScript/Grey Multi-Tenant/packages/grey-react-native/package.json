  },
  "peerDependenciesMeta": {
    "@react-native-async-storage/async-storage": {
      "optional": true
    }
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0",
