{
  "name": "@grey/react-native",
  "version": "0.1.0",
  "description": "Grey Multi-Tenant React Native bindings",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
