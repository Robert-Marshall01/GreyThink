{
  "name": "@grey/electron",
  "version": "0.1.0",
  "description": "Grey Multi-Tenant Electron bindings",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./main": {
      "types": "./dist/main/index.d.ts",
      "import": "./dist/main/index.js"
    },
    "./preload": {
      "types": "./dist/preload/index.d.ts",
      "import": "./dist/preload/index.js"
    },
    "./renderer": {
      "types": "./dist/renderer/index.d.ts",
      "import": "./dist/renderer/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@grey/adapters": "workspace:*",
    "@grey/core-client": "workspace:*"
  },
  "peerDependencies": {
    "electron": "^28.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "electron": "^28.0.0",
    "react": "^18.2.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0"
  },
  "files": ["dist"],
  "license": "MIT"
}
